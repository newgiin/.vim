set nocompatible

syntax enable
colorscheme sublime
set guifont=Consolas:h11:cANSI

set nobackup

set ignorecase " ignore case for search
set smartcase  " ignore case if search is all lowercase, case-senstitive otherwise

set nu
set cursorline
set hlsearch

set tabstop=4
set shiftwidth=4
set shiftround
set expandtab
set autoindent
set smartindent
